// Geometry

nx 20;
ny 200;
nz 4;

// Solver settings

dt 0.005;
endT 30;
writeInt 1;

URF_p 0.3;
URF_all 0.5;

// Set fields

h0_water 1;

// RAS model

RASmodel kOmegaSSTSato;  // mixtureKEpsilon;  // kOmegaSSTSato

// Boundary Conditions

Uinlet uniform (0 0.01 0);
massFlowIn 2.37e-5;
inizioMedia 15;

// Force models

Dbubble 3e-3;
Dragmodel IshiiZuber;       // SchillerNaumann - IshiiZuber - TomiyamaAnalytic - TomiyamaCorrelated
Liftmodel Tomiyama;         // none - Tomiyama
Walllub Antal;              // none - Antal - Frank - Tomiyama
Turbdisp Burns;             // none - Burns - LopezDeBertodano

