// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


holdUp
    {
        type            volFieldValue;
        libs            ("libfieldFunctionObjects.so");

        log             true;
        writeControl    timeStep;
        writeInterval	10;
        writeFields     true;

        regionType      all;
        operation       weightedVolAverage;

        fields
        (
            alpha.air
        );
    }
