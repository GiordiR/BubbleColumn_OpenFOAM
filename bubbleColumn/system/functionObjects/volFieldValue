FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      functionObject;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

volFieldValue
{
	type            	volFieldValue;
    functionObjectLibs 	("libfieldFunctionObjects.so");
    enabled         	false;

	writeControl   		timeStep;
	writeInterval  		100;

    log             	true;
    writeFields     	false;

	regionType 			cellZone;
	name 				internalcells;

	operation 			none;

    fields
    (
        UMean.air
        UMean.water
        alphaMean.air
        alphaMean.water
        pMean
    );

}
