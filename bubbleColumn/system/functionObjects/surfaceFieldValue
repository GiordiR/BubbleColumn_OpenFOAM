FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      functionObject;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceFieldValue
{
    type           		surfaceFieldValue;
    functionObjectLibs 	("libfieldFunctionObjects.so");
    enabled         	false;

	writeControl   		timeStep;
	writeInterval  		100;
	
	surfaceFormat		raw;

    log             	true;
    writeFields     	false;

    regionType          faceZone;
    name      			internalfacepatch;

    operation       	none;
    
    fields
    (
        UMean.air
        UMean.water
        alphaMean.air
        alphaMean.water
        pMean
    );

}
