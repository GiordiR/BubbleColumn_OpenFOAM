//Geometry

nx  20;
ny  200;
nz  4;

// Solver settings

dt 0.005;
endT 0.02;
writeInt 0.01;


// Set fields

h0_water 0.8;

// RAS model

RASmodel mixtureKEpsilon; // kOmegaSSTSato

// Boundary Conditions

Uinlet uniform (0 0.006 0);
inizioMedia 0;

// Force models

Dbubble     3e-3;
Dragmodel   SchillerNaumann; //SchillerNaumann - IshiiZuber - TomiyamaAnalytic - TomiyamaCorrelated
Liftmodel   none; //none - Tomiyama
Walllub     none; //none - Antal - Frank - Tomiyama
Turbdisp    none; //none - Burns - LopezDeBertodano

