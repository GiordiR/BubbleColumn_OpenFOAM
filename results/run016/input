// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Geometry

nx 20;   // 20x200x4 - 20x200x8 - 40x200x8 - 40x400x8
ny 200;
nz 4;

// Solver settings

dt 0.005;   // 0.01 - 0.005 - 0.0025
endT 90;
writeInt 10;

URF_p 0.2;
URF_all 0.3;

// Set fields

h0_water 1;

// RAS model

RASmodel kOmegaSSTSato;  // mixtureKEpsilon - kOmegaSSTSato - LaheyKEpsilon

// Boundary Conditions

//Uinlet uniform (0 0.01 0);
massFlowIn 1.422e-5;   // 2.37e-5 1.896e-05 1.422e-05 kg/s
inizioMedia 30;

// Force models

Dbubble 3e-3;
Dragmodel IshiiZuber;       // SchillerNaumann - IshiiZuber - TomiyamaAnalytic - TomiyamaCorrelated
Liftmodel Tomiyama;         // none - Tomiyama
Walllub Antal;              // none - Antal - Frank - Tomiyama
Turbdisp Burns;             // none - Burns - LopezDeBertodano

